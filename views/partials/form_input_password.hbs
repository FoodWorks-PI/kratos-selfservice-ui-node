<div class="input-container">
    <span>{{getTitle name}}</span>
    <fieldset>
        <label>
            <input name="{{name}}" type="{{type}}" value="{{value}}" placeholder="{{getTitle name}}" {{#if disabled}}disabled{{/if}}>
            {{> form_input_password_toggle }}
        </label>
        {{> messages }}
    </fieldset>
</div>